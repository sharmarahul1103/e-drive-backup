spring:
  datasource:
    url: http://localhost:3304/securitydb
    driver-class-name: com.mysql.cj.jdbc.Driver
    username: myuser
    password: mypassword
  jpa:
    show-sql: true
    hibernate:
      ddl-auto: :update
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQLDialect
         
    
          